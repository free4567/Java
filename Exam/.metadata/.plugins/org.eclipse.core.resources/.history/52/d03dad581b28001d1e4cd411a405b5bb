package 평가;

public class 문제3 {

	public static void main(String[] args) {
		for (int i=1; i<6; i++) {
			
			for (int j=5; j>0; j--) {
				
					System.out.println(" ");
				}
		        
				for(int k=1; k<=(1*2)-1; k++) {
		        	System.out.println("☆");
		        }
				System.out.println();
		}
	}
}	